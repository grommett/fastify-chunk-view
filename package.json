{
  "name": "fastify-chunk-view",
  "version": "1.1.0",
  "description": "A Fastify plugin that chunks content as it becomes available.",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": {
      "require": "./cjs/index.cjs"
    }
  },
  "scripts": {
    "test": "tap",
    "lint": "eslint",
    "cjs": "rollup index.js --file cjs/index.cjs --format cjs --exports auto --external stream,fastify-plugin"
  },
  "keywords": [
    "fastify",
    "progressive html",
    "progressive render",
    "pssr"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fastify-plugin": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^8.5.0",
    "fastify": "^3.25.2",
    "rollup": "^2.62.0",
    "simple-get": "^4.0.0",
    "tap": "^15.1.5"
  }
}
